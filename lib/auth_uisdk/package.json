{
  "name": "@ninembs-studio/auth-ui",
  "version": "0.0.1-alpha.1",
  "private": false,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist node_modules",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./auth-header": {
      "import": "./dist/components/AuthHeader.js",
      "require": "./dist/components/AuthHeader.js",
      "types": "./dist/components/AuthHeader.d.ts"
    },
    "./auth-wrapper": {
      "import": "./dist/components/AuthWrapper.js",
      "require": "./dist/components/AuthWrapper.js",
      "types": "./dist/components/AuthWrapper.d.ts"
    },
    "./email-verification": {
      "import": "./dist/components/EmailVerification.js",
      "require": "./dist/components/EmailVerification.js",
      "types": "./dist/components/EmailVerification.d.ts"
    },
    "./forgot-password": {
      "import": "./dist/components/ForgotPassword.js",
      "require": "./dist/components/ForgotPassword.js",
      "types": "./dist/components/ForgotPassword.d.ts"
    },
    "./login": {
      "import": "./dist/components/Login.js",
      "require": "./dist/components/Login.js",
      "types": "./dist/components/Login.d.ts"
    },
    "./logout-button": {
      "import": "./dist/components/LogoutButton.js",
      "require": "./dist/components/LogoutButton.js",
      "types": "./dist/components/LogoutButton.d.ts"
    },
    "./registration": {
      "import": "./dist/components/Registration.js",
      "require": "./dist/components/Registration.js",
      "types": "./dist/components/Registration.d.ts"
    },
    "./reset-password": {
      "import": "./dist/components/ResetPassword.js",
      "require": "./dist/components/ResetPassword.js",
      "types": "./dist/components/ResetPassword.d.ts"
    }
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.1.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.10",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.471.0",
    "react-haiku": "^2.1.8",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "^5"
  },
  "files": [
    "dist",
    "readme.md"
  ],
  "author": "ninembs studio llc",
  "license": "UNLICENSED",
  "description": "auth components"
}
